[% FOREACH pr IN projects -%]
<div class=mcell_row>
 <div class=mcell>
  <h2>[% pr.name %]</h2>
  <div class="mcell_content">
  [% FOREACH rr IN rref -%]
    [% IF rr.project_id == pr.project_id %]
      <a href="[% c.uri_for( '/br/' _ pr.name _ '/' _ rr.rref_name.replace('/','--') ) %]">[% rr.rref_name %]</a>
      <br>
    [% END %]
  [% END %]
  </div>
 </div>
</div>
[% IF loop.count() % 3 == 0 %]<br style="clear:both;">[% END %]
[% END %]
</div>

